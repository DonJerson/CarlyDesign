{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "css.styleSheets": ["css/*.css"],

  "files.associations": {
    // extension name : html
    "*.ejs": "html",
    "*.js": "html"
  },
  "editor.wordWrap": "on",
  "liveServer.settings.port": 5501
}
